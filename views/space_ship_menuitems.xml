<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="spaceship_list_action" model="ir.actions.act_window">
        <field name="name">Space Ship</field>
        <field name="res_model">spaceship.spaceship</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the first Space Ship
            </p>
        </field>
    </record>

    <menuitem name="Space Ship" id="menu_spaceship" sequence="1" web_icon="static/description/icon.png">
        <menuitem id="menu_spaceship_sub" name="space Ship" sequence="1">
            <menuitem id="menu_spaceship" name="SpaceShips" sequence="2" action="spaceship_list_action"/>
        </menuitem>
    </menuitem>
</odoo>